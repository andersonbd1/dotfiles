{
    "diffEditor.renderIndicators": false,
    "diffEditor.renderSideBySide": false,
    "editor.cursorBlinking": "solid",
    "editor.fontFamily": "SF Mono, Vazir Code",
    "editor.fontLigatures": true,
    "editor.fontSize": 14,
    "editor.formatOnPaste": true,
    "editor.hideCursorInOverviewRuler": true,
    "editor.lineHeight": 20,
    "editor.minimap.enabled": false,
    "editor.multiCursorModifier": "ctrlCmd",
    "editor.renderIndentGuides": true,
    "editor.renderLineHighlight": "gutter",
    "editor.selectionHighlight": false,
    "editor.smoothScrolling": true,
    "editor.snippetSuggestions": "top",
    "editor.tabSize": 2,
    "eslint.autoFixOnSave": true,
    "eslint.run": "onSave",
    "explorer.openEditors.visible": 0,
    "extensions.autoUpdate": false,
    "extensions.ignoreRecommendations": true,
    "files.associations": { "*.note": "markdown" },
    "files.insertFinalNewline": true,
    "files.trimTrailingWhitespace": true,
    "flow.useNPMPackagedFlow": true,
    "git.confirmSync": false,
    "javascript.implicitProjectConfig.checkJs": true,
    "prettier.jsxBracketSameLine": true,
    "prettier.printWidth": 100,
    "prettier.singleQuote": true,
    "prettier.trailingComma": "es5",
    "sublimeTextKeymap.promptV3Features": true,
    "telemetry.enableCrashReporter": false,
    "telemetry.enableTelemetry": false,
    "terminal.external.osxExec": "iTerm.app",
    "terminal.integrated.cursorStyle": "line",
    "terminal.integrated.fontSize": 14,
    "terminal.integrated.shell.osx": "/usr/local/bin/bash",
    "window.title": "${activeEditorMedium}${separator}${rootName}",
    "window.titleBarStyle": "native",
    "workbench.activityBar.visible": true,
    "workbench.colorTheme": "PlasticAB",
    "workbench.iconTheme": "vs-nomo-dark"
    // "vscode_custom_css.imports": ["file:///Users/allen/cloud/dotfiles/templates/vscode/custom.css"],
}
